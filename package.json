    "clsx": "^2.1.0",
    "fs-extra": "^11.2.0",
    "lucide-vue-next": "^0.368.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.7.0",
    "tailwind-merge": "^2.2.2",
    "vue": "^3.4.22",
    "vue-router": "^4.3.0"
